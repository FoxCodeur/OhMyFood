        "src": "sass/main.scss",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "assets/css"
        }
      }
    ],
    "routes": [
      {
        "src": "/assets/css/main.css",
        "dest": "/assets/css/main.css"
      }
    ]
  }
  